{
  "manifest_version": 3,
  "name": "Brightspace Downloader",
  "version": "1.0",
  "description": "Downloaded Lectures on brightspace",
  "permissions": [
    "scripting",
    "activeTab",
    "tabs",
    "debugger",
    "declarativeContent"
  ],
  "host_permissions": ["https://brightspace.carleton.ca/d2l/le/content/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon128.png"
    }
  }
}
