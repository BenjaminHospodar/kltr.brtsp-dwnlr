{
  "manifest_version": 3,
  "name": "Brightspace Downloader",
  "version": "1.0",
  "description": "Downloaded Lectures on brightspace",
  "permissions": ["scripting","activeTab","tabs","downloads","debugger"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ]
}
