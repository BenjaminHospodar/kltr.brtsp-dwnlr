{
  "manifest_version": 3,
  "name": "Get Tab Title",
  "version": "1.0",
  "description": "Pulls the title of the current tab.",
  "permissions": ["scripting","activeTab","debugger","downloads"],
  "host_permissions": ["<all_urls>"],

  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon128.png"
    }
  }
}
